<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
      
      <record model="ir.ui.view" id="real_estate_top_form_publication_urbanizacom">
          <field name="name">real.estate.top.form.publication.urbanizacom</field>
          <field name="model">real.estate.top</field>
          <field name="type">form</field>
          <field name="inherit_id" ref="real_estate_publication.real_estate_top_form_publication"/>
          <field name="arch" type="xml">
                <field name="internet_wo" position="before">
                    <field name="urbanizacom"/>
                </field>
          </field>
      </record>
      
        <record model="ir.ui.view" id="real_estate_top_form_publication_common_urbanizacom">
          <field name="name">real.estate.top.form.publication.common.urbanizacom</field>
          <field name="model">real.estate.top</field>
          <field name="type">form</field>
          <field name="inherit_id" ref="real_estate_internet_common.real_estate_top_form_publication_common"/>
          <field name="arch" type="xml">
                <field name="door" position="after">
                    <group colspan="4" col="4" >
                        <separator string="Urbaniza" colspan="4"/>
                        <field name="urbaniza_street_type" />
                        <field name="urbaniza_piso" />
                        <field name="urbaniza_bank" />
                        <field name="urbaniza_vpo" />
                        <field name="urbaniza_type" />
                        <separator colspan="4"/>
                        <field name="urbaniza_fee_type" />
                        <field name="urbaniza_fee_captador" />
                        <field name="urbaniza_fee_colaborador" />
                        <field name="urbaniza_commission_refer" />
                        <separator colspan="4"/>
                        <field name="urbaniza_exclusivity_doc" filename="urbaniza_exclusivity_doc_filename" />
                        <field name="capt_date" />
                        <field name="end_date" />
                    </group>
                </field>
          </field>
      </record>
      
      <record model="ir.ui.view" id="real_estate_top_filter_common_urbaniza">
          <field name="name">real.estate.top.filter.common.urbaniza</field>
          <field name="model">real.estate.top</field>
          <field name="type">search</field>
          <field name="inherit_id" ref="real_estate_internet_common.real_estate_top_filter_common"/>
          <field name="arch" type="xml">
                <group name="internet" position="inside">
                    <filter string="Publish in urbaniza.com" domain="[('urbanizacom','=',True)]"/>
                </group>
          </field>
      </record>

