<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
      
      <record model="ir.ui.view" id="real_estate_top_form_publication_inmoagrupacom">
          <field name="name">real.estate.top.form.publication.inmoagrupacom</field>
          <field name="model">real.estate.top</field>
          <field name="type">form</field>
          <field name="inherit_id" ref="real_estate_publication.real_estate_top_form_publication"/>
          <field name="arch" type="xml">
                <field name="internet_wo" position="before">
                    <field name="inmoagrupacom"/>
                </field>
          </field>
      </record>
      
        <record model="ir.ui.view" id="real_estate_top_form_publication_common_inmoagrupacom">
          <field name="name">real.estate.top.form.publication.common.inmoagrupacom</field>
          <field name="model">real.estate.top</field>
          <field name="type">form</field>
          <field name="inherit_id" ref="real_estate_internet_common.real_estate_top_form_publication_common"/>
          <field name="arch" type="xml">
                <field name="door" position="after">
                    <group colspan="4" col="4" >
                        <separator string="Urbaniza-Inmoagrupa" colspan="4"/>
                        <field name="inmoagrupacom_street_type" />
                        <field name="inmoagrupacom_piso" />
                        <field name="inmoagrupacom_bank" />
                        <field name="inmoagrupacom_vpo" />
                        <field name="inmoagrupacom_type" />
                        <separator colspan="4"/>
                        <field name="inmoagrupacom_fee_type" />
                        <field name="inmoagrupacom_fee_captador" />
                        <field name="inmoagrupacom_fee_colaborador" />
                        <field name="inmoagrupacom_commission_refer" />
                        <separator colspan="4"/>
                        <field name="inmoagrupacom_exclusivity_doc" />
                        <field name="inmoagrupacom_exclusivity_doc_url" widget="url" readonly="1"/>
                        <field name="capt_date" />
                        <field name="end_date" />
                    </group>
                </field>
          </field>
      </record>
      
